{
  "name": "Custom sorting of google search results",
  "description": "Custom sorting of google search results",
  "version": "0.10",
  "manifest_version": 2,
  "author": "SokolovDADeveloper",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search?*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}